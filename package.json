{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "amano-discord-bot",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"lint": "prettier --check . && eslint --ext .js,.mjs,.cjs src",
