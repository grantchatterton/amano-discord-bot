		"discord.js": "^14.16.0",
		"dotenv": "^16.6.1",
		"mysql2": "^3.15.0",
		"openai": "^5.22.0",
		"sequelize": "^6.37.7",
		"sqlite3": "^5.1.7",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"eslint": "^8.57.0",
		"eslint-config-neon": "^0.1.62",
		"eslint-formatter-pretty": "^6.0.1",
		"prettier": "^3.6.2",
		"sequelize-cli": "^6.6.3"
	}
}
