{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "amano-discord-bot",
	"version": "1.8.0",
	"private": true,
	"type": "module",
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"docs:commands": "node src/util/generateCmdDocs.js",
		"lint": "eslint --ext .js,.mjs,.cjs src",
		"lint:fix": "eslint --ext .js,.mjs,.cjs --fix src",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"start": "node --require dotenv/config src/index.js",
		"deploy": "node --require dotenv/config src/util/deploy.js",
		"test": "echo \"No tests specified!\""
	},
	"dependencies": {
		"@discordjs/core": "^1.2.0",
		"axios": "^1.12.2",
		"discord.js": "^14.16.0",
		"dotenv": "^16.6.1",
		"mysql2": "^3.15.0",
		"openai": "^5.22.0",
		"quick-lru": "^7.3.0",
		"sequelize": "^6.37.7",
		"sqlite3": "^5.1.7",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"eslint": "^8.57.0",
		"eslint-config-neon": "^0.1.62",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.6",
		"markdown-table-ts": "^1.0.3",
		"prettier": "^3.6.2",
		"semantic-release": "^25.0.1",
		"sequelize-cli": "^6.6.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,mjs,cjs}": [
			"eslint --fix",
			"prettier --write"
		]
	}
}
